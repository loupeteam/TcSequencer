<TcPlcObject Version="1.1.0.1">
  <POU Name="FB_StepManagerBasic_Test" Id="{f2f4fe9e-d358-482a-b327-02ec29657bd0}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_StepManagerBasic_Test EXTENDS FB_TestSuite
VAR_INPUT
